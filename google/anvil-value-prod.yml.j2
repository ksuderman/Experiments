galaxy:
  configs:
    job_conf.yml:
      execution:
        environments:
          tpv_dispatcher:
            tpv_config_files: {https://raw.githubusercontent.com/galaxyproject/tpv-shared-database/main/tools.yml,lib/galaxy/jobs/rules/tpv_rules_local.yml,https://gist.githubusercontent.com/afgane/68d1dbbe0af2468ba347dc74b6d3f7fa/raw/20edda50161bdcb74ff38935e7f76d79bfdaf303/tvp_rules_tests.yml}
cvmfs:
  cvmfscsi:
    cache:
      alien:
        pvc:
          storageClass: "nfs-{{ prefix }}-1"
galaxy:
  configs:
    "galaxy.yml":
      galaxy:
        admin_users: "tests@fake.org"
        master_api_key: {{ password }}
        single_user: "tests@fake.org"
    "job_conf.yml":
      runners:
        k8s:
          k8s_node_selector: "cloud.google.com/gke-nodepool: default-pool"
  image:
    version: 22.05
  ingress:
    ingressClassName: ""
  nodeSelector:
    "cloud.google.com/gke-nodepool": "default-pool"
  persistence:
    storageClass: "nfs-{{ prefix }}-1"
  postgresql:
    galaxyDatabasePassword: {{ password }}
    master:
      nodeSelector:
        "cloud.google.com/gke-nodepool": "default-pool"
    persistence:
      existingClaim: "{{ prefix }}-1-postgres-disk-pvc"
  rabbitmq:
    nodeSelector:
      "cloud.google.com/gke-nodepool": "default-pool"
    persistence:
      storageClassName: "nfs-{{ prefix }}-1"
  service:
    type: LoadBalancer
  tusd:
    ingress:
      ingressClassName: ""
nfs:
  nodeSelector:
    "cloud.google.com/gke-nodepool": "default-pool"
  persistence:
    existingClaim: "{{ prefix }}-1-nfs-disk-pvc"
  storageClass:
    name: "nfs-{{ prefix }}-1"
persistence:
  nfs:
    name: "{{ prefix }}-1-nfs-disk"
    persistentVolume:
      extraSpec:
        gcePersistentDisk:
          pdName: "{{ prefix }}-1-nfs-pd"
  postgres:
    name: "{{ prefix }}-1-postgres-disk"
    persistentVolume:
      extraSpec:
        gcePersistentDisk:
          pdName: "{{ prefix }}-1-postgres-pd"

